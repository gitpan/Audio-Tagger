FileRef *				TL_OBJ

OUTPUT
TL_OBJ
	sv_setref_pv($arg, \"Audio::Tagger::${subtype}\", (void *) $var);

INPUT
TL_OBJ
	if (sv_isobject($arg) && sv_derived_from($arg, \"Audio::Tagger::${subtype}\"))
		$var = INT2PTR($type, SvIV((SV *) SvRV($arg)));
	else
		Perl_croak(aTHX_ \"$var is not of type Audio::Tagger::${subtype}\")
